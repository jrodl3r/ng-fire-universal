{
  "name": "ng-fire-universal",
  "version": "0.9.0",
  "repository": "https://github.com/jrodl3r/ng-fire-universal",
  "author": "John Rodler",
  "license": "MIT",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "build": "ng build",
    "build:ssr": "npm run build:bundles && npm run compile:server",
    "build:bundles": "ng build --prod && ng run ng-fire-universal:server:production",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "start": "npm run serve:ssr",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "gcloud app deploy",
    "deploy:functions": "firebase deploy --only functions",
    "init:functions": "cd functions && npm i && cd .. && npm run deploy:functions"
  },
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/platform-server": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@nguniversal/express-engine": "^7.0.2",
    "@nguniversal/module-map-ngfactory-loader": "v7.0.2",
    "express": "^4.15.2",
    "firebase": "^6.2.2",
    "ngx-toastr": "^10.0.4",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.3",
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.4",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.6",
    "@types/jest": "^24.0.15",
    "@types/node": "~8.9.4",
    "bufferutil": "^4.0.1",
    "codelyzer": "^5.0.0",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.8.0",
    "protractor": "~5.4.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "utf-8-validate": "^5.0.2",
    "webpack-cli": "^3.1.0",
    "ws": "^7.0.1",
    "xhr2": "^0.2.0"
  }
}
